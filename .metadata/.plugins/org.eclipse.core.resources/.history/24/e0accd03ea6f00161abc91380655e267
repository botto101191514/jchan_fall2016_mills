
public class DoMath {

	public static void main(String[] args) {
		System.out.println("squares" + Calculate.square(5));
		System.out.println("cubed" + Calculate.cube(5));
		System.out.println("average of 2 numbers" + Calculate.average2(32,4));
		System.out.println("average of 3 numbers:" + Calculate.average3(32, 5, 27));
	}
}